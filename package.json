{
  "name": "uri-change-observer",
  "description": "Observe changes on web resources",
  "version": "0.0.1",
  "keywords": [
    "uri",
    "observer"
  ],
  "author": {
    "name": "Samuel Campos",
    "url": "http://samuelcampos.net/"
  },
  "license": "MIT",
  "main": "./distribution/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelcampos/uri-change-observer.git"
  },
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir distribution",
    "check": "npm run eslint && npm run test",
    "coverage": "istanbul cover _mocha test -- -R spec",
    "eslintSource": "./node_modules/.bin/eslint src/*",
    "eslintTest": "./node_modules/.bin/eslint test/*",
    "eslint": "npm run eslintSource && npm run eslintTest",
    "preparePublish": "npm run check && npm run build",
    "test": "mocha test --recursive --compilers js:babel-core/register"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "node-schedule": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-plugin-jasmine": "^1.8.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
